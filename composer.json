{
    "name": "latuconsinafr/3d-bin-packager",
    "description": "PHP Library for 3D Bin Packing.",
    "keywords": [
        "php",
        "bin-packing",
        "2d-bin-packing",
        "3d-bin-packing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Farista Latuconsina",
            "email": "faristalatuconsina@gmail.com",
            "homepage": "https://github.com/latuconsinafr"
        },
        {
            "name": "Maksym Medar",
            "email": "max@medar.pp.ua"
        }
    ],
    "require": {
        "php": "^5.6",
      "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Latuconsinafr\\BinPackager\\BinPackager3D\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "scripts": {
        "test": "phpunit --testdox --configuration phpunit.xml --colors=always",
        "test:coverage": "php -d xdebug.mode=coverage -r \"require 'vendor/bin/phpunit';\" -- --configuration phpunit.xml --colors=always --coverage-clover build/logs/clover.xml --coverage-html build/coverage"
    }
}